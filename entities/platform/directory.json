{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Directory",
  "description": "Represents a Directory (Partner) account in the platform",
  "type": "object",
  "properties": {
    "uid": {
      "type": "string",
      "description": "Entity's unique identifier"
    },
    "name": {
      "type": "string",
      "description": "The name of the directory"
    },
    "subdomain": {
      "type": "string",
      "description": "The vcita subdomain associated with the directory"
    },
    "whitelabel_subdomain": {
      "type": "boolean",
      "description": "Whether whitelabel subdomain is enabled"
    },
    "host_mapping": {
      "type": "string",
      "description": "The custom domain mapped to the directory"
    },
    "is_managed": {
      "type": "boolean",
      "description": "Indicates if the directory is a managed partner"
    },
    "is_deprecated": {
      "type": "boolean",
      "description": "Indicates if the directory is deprecated"
    },
    "manual_type": {
      "type": "string",
      "enum": [
        "white_label",
        "solution_provider",
        "co_branded"
      ],
      "description": "The type of partnership/directory"
    },
    "configuration_key": {
      "type": "string",
      "description": "Internal configuration key (e.g., 'duda', 'star', 'vcita')"
    },
    "billing_type": {
      "type": "string",
      "enum": [
        "manual",
        "recurly"
      ],
      "description": "The billing method used for this directory"
    },
    "locale": {
      "type": "string",
      "description": "The default locale for the directory"
    },
    "time_zone": {
      "type": "string",
      "description": "The time zone of the directory"
    },
    "home_url": {
      "type": "string",
      "description": "The home URL of the partner portal"
    },
    "support_url": {
      "type": "string",
      "description": "Support contact URL or email"
    },
    "faq_url": {
      "type": "string",
      "description": "FAQ or help center URL"
    },
    "cancel_page_url": {
      "type": "string",
      "description": "URL for the subscription cancellation page"
    },
    "upgrade_page_url": {
      "type": "string",
      "description": "URL for the subscription upgrade page"
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "description": "Entity creation time (ISO 8601)"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "description": "Entity update time (ISO 8601)"
    }
  },
  "readOnly": [
    "uid",
    "created_at",
    "updated_at"
  ],
  "required": [
    "uid",
    "name"
  ],
  "example": {
    "uid": "D42sDg343mdfDLqasTsHDtDRq2DedCSA",
    "name": "Acme Partner",
    "subdomain": "acme-portal",
    "whitelabel_subdomain": true,
    "host_mapping": "portal.acme.com",
    "is_managed": true,
    "is_deprecated": false,
    "manual_type": "white_label",
    "configuration_key": "vcita",
    "billing_type": "recurly",
    "locale": "en-US",
    "time_zone": "America/New_York",
    "home_url": "https://portal.acme.com/home",
    "support_url": "support@acme.com",
    "faq_url": "https://faq.acme.com",
    "created_at": "2024-01-10T10:00:00Z",
    "updated_at": "2024-05-15T12:30:00Z"
  }
}
