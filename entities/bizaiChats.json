{
  "type": "object",
  "properties": {
    "uid": {
      "type": "string",
      "description": "uid of the chat",
      "example": "d290f1ee-6c54-4b01-90e6-d701748f0851"
    },
    "actor_uid": {
      "type": "string",
      "description": "uid of the actor that created the chat",
      "example": "ghp0f1ee-6c54-4b01-90e6-d701748f0851"
    },
    "agent": {
      "type": "string",
      "description": "Agent associated with the chat",
      "example": "vanilla"
    },
    "metadata": {
      "type": "object",
      "description": "metadata of the chat",
      "example": {
        "directory_uid": "k98axtpqg7h1whh6",
        "instruction": "You are a helpful assistant. Answer all questions to the best of your ability."
      }
    },
    "created_at": {
      "type": "string",
      "description": "Date the chat was created",
      "format": "date-time",
      "example": "2024-05-20T14:00:00.000Z"
    },
    "updated_at": {
      "type": "string",
      "description": "Date the chat was last updated",
      "format": "date-time",
      "example": "2024-05-20T14:00:00.000Z"
    }
  },
  "required": [
    "uid",
    "actor_uid",
    "agent",
    "metadata",
    "created_at",
    "updated_at"
  ],
  "example": {
    "uid": "d290f1ee-6c54-4b01-90e6-d701748f0851",
    "actor_uid": "ghp0f1ee-6c54-4b01-90e6-d701748f0851",
    "agent": "biz-ai",
    "metadata": {},
    "created_at": "2024-07-20T14:00:00.000Z",
    "updated_at": "2024-05-20T14:00:00.000Z"
  }
}
