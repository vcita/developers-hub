{
  "type": "object",
  "required": [
    "businesses"
  ],
  "properties": {
    "businesses": {
      "type": "array",
      "description": "List of businesses with their availability information",
      "items": {
        "type": "object",
        "required": [
          "booking_slots",
          "booking_widget_url",
          "merchant_id"
        ],
        "properties": {
          "booking_slots": {
            "type": "array",
            "description": "Array of available booking time slots in ISO format",
            "items": {
              "type": "string",
              "format": "date-time"
            }
          },
          "booking_widget_url": {
            "type": "string",
            "description": "URL for the business's booking widget"
          },
          "merchant_id": {
            "type": "string",
            "description": "Unique identifier for the merchant/business"
          }
        }
      }
    }
  },
  "example": {
    "businesses": [
      {
        "booking_slots": [
          "2023-03-15T09:00:00Z",
          "2023-03-15T09:30:00Z",
          "2023-03-15T10:00:00Z"
        ],
        "booking_widget_url": "https://booking.vcita.com/business1234",
        "merchant_id": "business1234"
      },
      {
        "booking_slots": [
          "2023-03-15T14:00:00Z",
          "2023-03-15T14:30:00Z",
          "2023-03-15T15:00:00Z"
        ],
        "booking_widget_url": "https://booking.vcita.com/business5678",
        "merchant_id": "business5678"
      }
    ]
  }
} 