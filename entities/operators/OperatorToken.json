{
    "type": "object",
    "properties": {
      "exp":{
        "type":"string",
        "description": "(expiration time): Time after which the JWT expires",
        "format": "date-time"
      },
      "iat":{
        "type":"string",
        "Description": "(issued at time): Time at which the JWT was issued; can be used to determine age of the JWT"
      },
      "sub":{
        "type":"string",
        "Description": "(subject): Subject of the JWT (the user)"
      },
      "type":{
        "type":"string",
        "description":"The type of the issued token. Can be 'operator' - for operators acting as himself, 'staff' - operator acting onbehalf of a business user",
        "enum":[
            "operator",
            "staff"
        ],
        "default":"operator"
      },
      "client_id":{
        "type":"string",
        "Description": "Staff unique id. Null when type is operator; mandatory when type is staff"
      },
      "internal_jwt":{
        "type":"string",
        "Description": "The token issued for the operator"
      }
    },
    "required": [
        "exp",
        "iat",
        "sub",
        "type",
        "internal_jwt"
    ],
