{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "StaffAiRecommendationsSettings", 
  "description": "AI recommendations settings configuration for a staff member",
  "type": "object",
  "properties": {
    "enable": {
      "type": "boolean",
      "description": "Whether AI recommendations are enabled for this staff member"
    },
    "sub_options": {
      "$ref": "https://vcita.github.io/developers-hub/entities/ai/staffAiRecommendationsSubOptions.json"
    }
  },
  "required": ["enable", "sub_options"],
  "example": {
    "enable": true,
    "sub_options": {
      "next_best_action": true,
      "estimate": false,
      "scheduling": true
    }
  }
}
