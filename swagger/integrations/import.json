{
  "openapi": "3.0.0",
  "info": {
    "title": "Import API",
    "description": "API for managing data imports",
    "version": "1.0",
    "contact": {}
  },
  "servers": [],
  "tags": [
    {
      "name": "entity-import",
      "description": "Trigger an entity import process"
    },
    {
      "name": "import-status",
      "description": "Import job status operations"
    },
    {
      "name": "import-details",
      "description": "Import details operations"
    }
  ],
  "components": {
    "schemas": {
      "Response": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "data": {
            "type": "object"
          }
        },
        "required": [
          "success",
          "data"
        ]
      },
      "ExcelProviderDataDto": {
        "type": "object",
        "properties": {
          "file": {
            "type": "string",
            "format": "binary",
            "description": "The Excel file to be imported"
          },
          "fields": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Optional array of field names to process"
          },
          "context": {
            "type": "object",
            "additionalProperties": true,
            "description": "Optional context data for the import"
          }
        },
        "required": [
          "file"
        ]
      },
      "UploadEntityRequestDto": {
        "type": "object",
        "properties": {
          "provider_type": {
            "type": "string",
            "enum": [
              "excel",
              "mock"
            ],
            "description": "The type of import provider"
          },
          "provider_data_uid": {
            "type": "string",
            "description": "Reference to previously created provider data (optional)"
          },
          "validate": {
            "type": "boolean",
            "default": false,
            "description": "If true, only validates the import without processing"
          },
          "excel_provider_data": {
            "$ref": "#/components/schemas/ExcelProviderDataDto",
            "description": "Optional Excel-specific provider data configuration"
          }
        },
        "required": [
          "provider_type"
        ]
      },
      "JobIdentifierDto": {
        "type": "object",
        "properties": {
          "job_uid": {
            "type": "string",
            "description": "The unique identifier of the created import job"
          }
        },
        "required": [
          "job_uid"
        ]
      },
      "ImportJobDto": {
        "$ref": "https://vcita.github.io/developers-hub/entities/integrations/import/ImportJob.json"
      },
      "ImportDetailsResponseDto": {
        "$ref": "https://vcita.github.io/developers-hub/entities/integrations/import/ImportDetails.json"
      },
      "MultiImportDetailsResponseDto": {
        "type": "object",
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "$ref": "https://vcita.github.io/developers-hub/entities/integrations/import/ImportDetails.json"
            }
          }
        },
        "required": [
          "items"
        ]
      }
    }
  },
  "paths": {
    "/v3/import/{domain}/{entity_type}": {
      "post": {
        "tags": [
          "entity-import"
        ],
        "summary": "Import data for a specific domain and entity type",
        "operationId": "importData",
        "parameters": [
          {
            "name": "domain",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "entity_type",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/UploadEntityRequestDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Import job created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Response"
                    },
                    {
                      "properties": {
                        "data": {
                          "$ref": "#/components/schemas/JobIdentifierDto"
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/v3/import/jobs/{job_uid}": {
      "get": {
        "tags": [
          "import-status"
        ],
        "summary": "Get import job status",
        "operationId": "getImportJob",
        "parameters": [
          {
            "name": "job_uid",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Import job details",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Response"
                    },
                    {
                      "properties": {
                        "data": {
                          "$ref": "#/components/schemas/ImportJobDto"
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/v3/import/job_items": {
      "get": {
        "tags": [
          "import-details"
        ],
        "summary": "Get all import job items",
        "operationId": "findAll",
        "parameters": [
          {
            "name": "page",
            "required": false,
            "in": "query",
            "description": "Page number of results.",
            "schema": {
              "minimum": 1,
              "maximum": 10000,
              "default": 1,
              "type": "number"
            }
          },
          {
            "name": "per_page",
            "required": false,
            "in": "query",
            "description": "How many items to return per page. Max: 100",
            "schema": {
              "minimum": 1,
              "maximum": 100,
              "default": 25,
              "type": "number"
            }
          },
          {
            "description": "Sort by fields. Example: sort=created_at:asc,updated_at:desc",
            "name": "sort",
            "required": false,
            "in": "query",
            "schema": {
              "default": "updated_at:desc",
              "type": "string"
            }
          },
          {
            "description": "Filter by a field. Example: filter[created_at][gt]=2019-01-01, filter[name]=my-name",
            "required": false,
            "name": "filter",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of import job items",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Response"
                    },
                    {
                      "properties": {
                        "data": {
                          "$ref": "#/components/schemas/MultiImportDetailsResponseDto"
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/v3/import/job_items/{uid}": {
      "get": {
        "tags": [
          "import-details"
        ],
        "summary": "Get a specific import job item",
        "operationId": "findOne",
        "parameters": [
          {
            "name": "uid",
            "required": true,
            "in": "path",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Import job item details",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/Response"
                    },
                    {
                      "properties": {
                        "data": {
                          "$ref": "#/components/schemas/ImportDetailsResponseDto"
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}