{
  "name": "openapi-unifier",
  "version": "1.0.0",
  "description": "Script to unify OpenAPI JSON files by domain with path normalization",
  "main": "unify-openapi.js",
  "scripts": {
    "unify": "node scripts/unify-openapi.js",
    "unify:verbose": "node scripts/unify-openapi.js --verbose",
    "unify:dry-run": "node scripts/unify-openapi.js --dry-run --verbose",
    "gen:entities": "node scripts/generate-entity-md.js",
    "gen:entity": "node scripts/generate-entity-md.js --entity",
    "update-readme": "node scripts/update-readme-swaggers.js",
    "update-readme:verbose": "node scripts/update-readme-swaggers.js --verbose",
    "update-readme:dry-run": "node scripts/update-readme-swaggers.js --dry-run --verbose",
    "update-readme:force": "node scripts/update-readme-swaggers.js --force --verbose",
    "validate": "node api-validation/src/cli/index.js",
    "validate:domain": "node api-validation/src/cli/index.js --domain",
    "validate:dry-run": "node api-validation/src/cli/index.js --dry-run",
    "validate:strict": "node api-validation/src/cli/index.js --strict",
    "validate:ui": "node api-validation/src/server/index.js",
    "setup:business": "node api-validation/scripts/setup-business.js",
    "setup:business:dry-run": "node api-validation/scripts/setup-business.js --dry-run",
    "setup:offering": "node api-validation/scripts/setup-offering.js",
    "setup:offering:dry-run": "node api-validation/scripts/setup-offering.js --dry-run",
    "migrate:workflows": "node api-validation/scripts/migrate-workflow-swagger.js",
    "migrate:workflows:dry-run": "node api-validation/scripts/migrate-workflow-swagger.js --dry-run --verbose",
    "migrate:workflows:verbose": "node api-validation/scripts/migrate-workflow-swagger.js --verbose"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "rdme": "^8.6.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@apidevtools/swagger-parser": "^10.1.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "jest": "^29.7.0",
    "mime-types": "^2.1.35",
    "openai": "^6.14.0"
  },
  "keywords": [
    "openapi",
    "swagger",
    "api",
    "unify",
    "merge"
  ],
  "author": "",
  "license": "ISC"
}
