{
  "swaggerPath": "mcp_swagger",
  "baseUrl": "https://app.meet2know.com/apigw",
  "options": {
    "timeout": 30000,
    "runDelete": false,
    "stopOnDocIssue": true,
    "retryOnTimeout": 2
  },
  "rateLimit": {
    "requestsPerSecond": 5,
    "maxConcurrent": 3,
    "delayBetweenRequests": 200,
    "retryOn429": true,
    "maxRetries": 3,
    "backoffMultiplier": 2
  }
}
