{
  "swaggerPath": "mcp_swagger",
  "baseUrl": "https://app.meet2know.com/apigw",
  "fallbackUrl": "https://app.meet2know.com/api2",
  "options": {
    "timeout": 30000,
    "runDelete": false,
    "stopOnDocIssue": true,
    "retryOnTimeout": 2
  },
  "rateLimit": {
    "requestsPerSecond": 5,
    "maxConcurrent": 3,
    "delayBetweenRequests": 200,
    "retryOn429": true,
    "maxRetries": 3,
    "backoffMultiplier": 2
  },
  "ai": {
    "models": {
      "resolver": "gpt-4.1-nano",
      "paramGenerator": "gpt-4o-mini",
      "healer": "claude-sonnet-4-20250514",
      "docFixer": "claude-sonnet-4-20250514"
    },
    "maxIterations": 2,
    "autoFixSwagger": false
  }
}
