{
  "swaggerPath": "mcp_swagger",
  "baseUrl": "https://app.meet2know.com/apigw",
  "fallbackUrl": "https://app.meet2know.com/api2",
  "options": {
    "timeout": 30000,
    "runDelete": false,
    "stopOnDocIssue": true,
    "retryOnTimeout": 2
  },
  "rateLimit": {
    "requestsPerSecond": 5,
    "maxConcurrent": 3,
    "delayBetweenRequests": 200,
    "retryOn429": true,
    "maxRetries": 3,
    "backoffMultiplier": 2
  },
  "ai": {
    "provider": "anthropic",
    "model": {
      "anthropic": "claude-sonnet-4-20250514",
      "openai": "gpt-5.2"
    }
  }
}
